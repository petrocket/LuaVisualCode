set(TARGET_WITH_NAMESPACE "3rdParty::cppdap")
if (TARGET ${TARGET_WITH_NAMESPACE})
    return()
endif()

set(MY_NAME "cppdap")

set(${MY_NAME}_INCLUDE_DIR ${CMAKE_CURRENT_LIST_DIR}/cppdap/install/include)
set(${MY_NAME}_LIBS_DIR ${CMAKE_CURRENT_LIST_DIR}/cppdap/install/lib)
set(${MY_NAME}_LIBRARY_RELEASE ${${MY_NAME}_LIBS_DIR}/Release/${CMAKE_STATIC_LIBRARY_PREFIX}cppdap${CMAKE_STATIC_LIBRARY_SUFFIX})
set(${MY_NAME}_LIBRARY_DEBUG ${${MY_NAME}_LIBS_DIR}/Debug/${CMAKE_STATIC_LIBRARY_PREFIX}cppdap${CMAKE_STATIC_LIBRARY_SUFFIX})

add_library(${TARGET_WITH_NAMESPACE} STATIC IMPORTED GLOBAL)

set_target_properties(${TARGET_WITH_NAMESPACE} 
    PROPERTIES 
        IMPORTED_LOCATION_DEBUG "${${MY_NAME}_LIBRARY_DEBUG}"
        IMPORTED_LOCATION  "${${MY_NAME}_LIBRARY_RELEASE}")

ly_target_include_system_directories(TARGET ${TARGET_WITH_NAMESPACE} INTERFACE ${${MY_NAME}_INCLUDE_DIR})

target_link_libraries(${TARGET_WITH_NAMESPACE} 
    INTERFACE ${CMAKE_DL_LIBS}
)

set(${MY_NAME}_FOUND True)
